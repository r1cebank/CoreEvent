    "react-native-device-info": "^0.9.6",
    "react-native-dialogbox": "^0.6.2",
    "react-native-drawer": "^2.3.0",
    "react-native-easy-grid": "^0.1.6",
    "react-native-elements": "^0.6.2",
    "react-native-geocoder": "^0.4.5",
    "react-native-google-analytics-bridge": "^3.1.0",
    "react-native-i18n": "^0.1.1",
