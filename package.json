{
  "name": "CoreEvent",
  "version": "0.0.1",
  "description": "CoreEvent mobile app",
  "author": "Siyuan Gao <siyuangao@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "keywords": [
    "React",
    "Native"
  ],
  "ava": {
    "babel": "inherit",
    "files": [
      "tests/**/*.spec.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./tests/setup"
    ]
  },
  "scripts": {
    "test": "./node_modules/.bin/ava --verbose",
    "lint": "eslint src -f node_modules/eslint-html-reporter/reporter.js -o report.html",
    "start": "./node_modules/react-native/packager/packager.sh",
    "reactotron": "./node_modules/.bin/reactotron",
    "coverage": "./node_modules/.bin/nyc ./node_modules/.bin/ava",
    "coverage:report": "./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/ava",
    "coverage:upload": "./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prerelease": "react-native bundle --entry-file ./index.ios.js --platform ios --bundle-output ios/main.jsbundle --dev false --minify true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/r1cebank/CoreEvent"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2 <4"
  },
  "dependencies": {
    "apisauce": "^0.5.0",
    "lodash": "^4.15.0",
    "parse": "^1.9.2",
    "parse-react": "^0.5.2",
    "react": "15.3.2",
    "react-native": "^0.35.0",
    "react-native-animated-ptr": "^1.0.2",
    "react-native-camera": "git+https://github.com/lwansbrough/react-native-camera.git",
    "react-native-elements": "^0.6.2",
    "react-native-google-analytics-bridge": "^3.1.0",
    "react-native-i18n": "^0.1.1",
    "react-native-image-progress": "^0.6.0",
    "react-native-looped-carousel": "^0.1.3",
    "react-native-progress": "^3.1.0",
    "react-native-router-flux": "^3.36.0",
    "react-native-vector-icons": "^2.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "redux-persist": "^3.2.2",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-native": "1.9.0",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.14.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-r1cebank": "^0.0.1",
    "eslint-html-reporter": "^0.5.2",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-ava": "^3.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-lodash": "^1.10.3",
    "eslint-plugin-react": "^6.2.0",
    "mockery": "^1.7.0",
    "node-fetch": "^1.6.3",
    "nyc": "^8.1.0",
    "react-addons-test-utils": "15.3.1",
    "react-dom": "15.3.1",
    "react-native-mock": "^0.2.5",
    "reactotron": "^0.9.0",
    "redux-mock-store": "^1.2.0",
    "remote-redux-devtools": "^0.4.7",
    "sinon": "^1.17.5"
  }
}
